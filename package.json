{
  "name": "dev-test-node",
  "version": "1.0.0",
  "description": "Nodejs dev test",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "start-inspect": "nodemon --exec 'node --inspect -r ts-node/register src/index.ts'",
    "build": "tsc",
    "go": "ts-node src/index.ts",
    "go:prod": "node ./build/src/index.js",
    "test-watch": "jest --watchAll",
    "test": "jest"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ioredis": "^4.22.0",
    "jsonwebtoken": "^8.5.1",
    "promise-retry": "^2.0.1",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.19.4",
    "@types/jest": "^24.0.13",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^12.0.2",
    "@types/promise-retry": "^1.1.3",
    "@types/redis": "^2.8.28",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-security": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.4.5"
  }
}
